{
  "version": 2,
  "functions": {
    "app.py": {
      "excludeFiles": "{static/**,assets/**,tests/**,__pycache__/**,venv/**}"
    }
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}